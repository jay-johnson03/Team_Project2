<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.CheckBox?>


<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
    fx:controller="project2.project2.controllers.signupController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button text="Close" fx:id="close" onAction="#close" />
    <TextField fx:id="emailField" promptText="Enter your email" />
    <PasswordField fx:id="passwordField" promptText="Enter your password" />
    <TextField fx:id="nameField" promptText="Enter your name" />
    <CheckBox fx:id="isProfessorField" text="Are you a professor?" />
    <Button text="Sign Up" onAction="#signUp" />
</VBox>